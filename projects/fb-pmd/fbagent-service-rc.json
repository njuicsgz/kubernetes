{
   "kind":"ReplicationController",
   "apiVersion":"v1beta3",
   "metadata":{
      "name":"fbagent-service",
      "namespace": "facebook-pmd",
      "labels":{
         "name":"fbagent-service"
      }
   },
   "spec":{
      "replicas":1,
      "selector":{
         "name":"fbagent-service"
      },
      "template":{
         "metadata":{
	    "namespace": "facebook-pmd",
            "labels":{
               "name":"fbagent-service"
            }
         },
         "spec":{
            "containers":[
               {
                  "name":"fbagent-service",
                  "image":"xa.repo.ndp.com:5000/facebook_pmd/fbagent-services:v1.0.3",
                  "env":[
                        {"name": "ZK_ADDRESS", "value": "172.30.10.185:2181"},
			{"name": "BROKER_LIST", "value": "172.30.50.26:9092,172.30.50.34:9092"}
                  ],
                  "ports":[
                  ]
               }
            ]
         }
      }
   }
}
