{
   "kind":"ReplicationController",
   "apiVersion":"v1beta3",
   "metadata":{
      "name":"walle-web",
      "namespace": "fb-pmd-allen",
      "labels":{
         "name":"walle-web"
      }
   },
   "spec":{
      "replicas":1,
      "selector":{
         "name":"walle-web"
      },
      "template":{
         "metadata":{
	    "namespace": "fb-pmd-allen",
            "labels":{
               "name":"walle-web"
            }
         },
         "spec":{
            "containers":[
               {
                  "name":"walle-web",
                  "image":"xa.repo.ndp.com:5000/facebook_pmd/walle-web:1.2",
                  "env":[
                        {"name": "ZK_ADDRESS", "value": "172.30.10.185:42181"},
                        {"name": "WALLE_JAVA_ADDRESS", "value": "172.30.10.122:9999"},
                        {"name": "LC_ALL", "value": "en_US.UTF-8"},
                        {"name": "LANG", "value": "en_US.UTF-8"},
                        {"name": "LANGUAGE", "value": "en_US:en"}
                  ],
                  "ports":[
                  ]
               }
            ]
         }
      }
   }
}
