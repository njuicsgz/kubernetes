{
   "kind":"ReplicationController",
   "apiVersion":"v1beta3",
   "metadata":{
      "name":"zk",
      "namespace": "fb-pmd-allen"
   },
   "spec":{
      "replicas":1,
      "selector":{
         "name":"zk"
      },
      "template":{
         "metadata":{
	    "namespace": "fb-pmd-allen",
            "labels":{
               "name":"zk"
            }
         },
         "spec":{
            "containers":[
               {
                  "name":"zk",
                  "image":"xa.repo.ndp.com:5000/paas/zookeeper:3.4.6"
               }
            ]
         }
      }
   }
}
